{
  "name": "gopherjs-vecty-electron",
  "version": "0.0.1",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "build-main": "gopherjs build -o main.js",
    "build-content": "gopherjs build ./contents -o contents.js",
    "build": "npm-run-all -p -r build-main build-content"
