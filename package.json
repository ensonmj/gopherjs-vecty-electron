    "gopherjs",
    "vecty",
    "electron"
  ],
  "author": "ensonmj",
  "license": "MIT License",
  "devDependencies": {
    "npm-run-all": "^4.1.1"
  },
  "dependencies": {
    "electron": "~1.7.6"
  }
}
